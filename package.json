{
  "name": "epictask",
  "version": "0.1.27",
  "description": "AN electron app for dealing with those darn git issues",
  "main": "bin/epictask-start.js",
  "scripts": {
    "clean": "node ./etc/scripts/clean.js",
    "dev:clean-workspace": "node ./etc/scripts/clean-dev-electron.js",
    "dev:storybook": "./etc/scripts/configure-dev-env.sh && start-storybook -s ./ -p 6006",
    "dev:storybook-build": "build-storybook",
    "dev:kill": "pkill -9 Electron || pkill -9 electron",
    "dev:watch-workspace": "babel-node ./etc/scripts/watch/watch-workspace.js",
    "test": "gulp compile test",
    "test:watch": "gulp compile-watch",
    "start": "./node_modules/.bin/cross-env HOT=1 NODE_ENV=development ./node_modules/.bin/electron ./bin/epictask-start.js",
    "compile": "gulp compile",
    "compile:watch": "babel-node ./etc/scripts/compile-watch.js",
    "package": "babel-node ./etc/scripts/package.js",
    "package:all": "babel-node ./etc/scripts/package.js --win --linux",
    "package:make-icons": "babel-node ./etc/scripts/make-icons.js",
    "install:material-ui": "npm i material-ui@../material-ui/build",
    "install:app": "babel-node ./etc/scripts/package.js --install"
  },
  "build": {
    "appId": "com.densebrain.epictask",
    "copyright": "Copyright Â© 2016 Densebrain, Inc.",
    "productName": "Epictask",
    "squirrelWindows": {
      "msi": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerHeaderIcon": "build/icon.ico"
    },
    "linux": {
      "category": "Development",
      "packageCategory": "devel",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "mac": {
      "identity": "Densebrain, Inc",
      "icon": "build/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "extraFiles": [
      "build/**/*"
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{README.md,README,readme.md,readme,test}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.idea,appveyor.yml,.travis.yml,circle.yml}"
    ]
  },
  "directories": {
    "app": "dist/app",
    "buildResources": "build",
    "output": "dist/build"
  },
  "packaging": {
    "destination": "./target/packages",
    "platforms": [
      "darwin-x64"
    ],
    "platformResources": {
      "darwin": {
        "icon": "./build/icon.icns"
      }
    }
  },
  "browser": {
    "vertx": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/densebrain/epictask"
  },
  "author": {
    "email": "support@densebrain.com",
    "name": "Densebrain, Inc",
    "url": "http://densebrain.com"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "__TS_CONFIG__": {
        "target": "es5",
        "module": "commonjs"
      }
    },
    "scriptPreprocessor": "./node_modules/ts-jest/preprocessor.js",
    "testRegex": "packages\\/epic-tests.*AppStore\\.spec\\.(ts|tsx)$",
    "setupFiles": [
      "etc/mocha/test-setup.js"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./packages"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "engines": {
    "electron": "1.4.3"
  },
