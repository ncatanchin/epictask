  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "cross-env PACKAGE=true NODE_ENV=development electron-webpack",
    "build": "npm run compile && electron-builder -c.compression=store \"-c.mac.identity=Densebrain, Inc\"",
    "build:nosign": "npm run compile && electron-builder -c.compression=store -c.mac.identity=null",
    "build:dir": "npm run compile && electron-builder --dir -c.compression=store -c.mac.identity=null",
    "release": "npm version patch && npm run compile && electron-builder -c.compression=store \"-c.mac.identity=Densebrain, Inc\" --publish always",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "build": {
    "productName": "Epictask",
    "mac": {
      "publish": [
        "github"
      ]
    }
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/main",
      "webpackConfig": "etc/webpack/webpack.config.main.js"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "webpackConfig": "etc/webpack/webpack.config.renderer.js"
