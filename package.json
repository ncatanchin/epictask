{
  "name": "epictask",
  "version": "0.1.19",
  "description": "AN electron app for dealing with those darn git issues",
  "main": "dist/app/bin/epictask-start.js",
  "scripts": {
    "local-install": "./etc/scripts/local-install.sh",
    "clean": "node ./etc/scripts/clean.js",
    "clean-dev-electron": "node ./etc/scripts/clean-dev-electron.js",
    "sync-win-dev-scp": "scp -r ${PWD}/src/* 10.0.1.49:c:/users/jglanz/development/densebrain/epictask-workspace/epictask/src/",
    "sync-win-dev": "node_modules\\.bin\\sync-files.cmd --watch z:\\jglanz\\Development\\densebrain\\epictask-workspace\\epictask\\src .\\src",
    "sync-linux-dev": "./node_modules/.bin/sync-files --watch /mnt/hgfs/jglanz/Development/densebrain/epictask-workspace/epictask/src ./src",
    "test": "gulp compile test",
    "test-watch": "gulp compile-watch",
    "start-dev": "./node_modules/.bin/cross-env HOT=1 NODE_ENV=development ./node_modules/.bin/electron ./dist/MainEntry.js",
    "postinstall": "node ./etc/scripts/post-install.js",
    "compile": "gulp compile",
    "compile-watch": "node ./etc/scripts/compile-watch.js",
    "clean-electron": "./etc/scripts/clean-electron.sh",
    "storybook": "./etc/scripts/configure-dev-env.sh && start-storybook -s ./ -p 6006",
    "build-storybook": "build-storybook",
    "package": "babel-node ./etc/scripts/package.js",
    "package-all": "node ./etc/scripts/package.js --win --linux",
    "make-icons": "node ./etc/scripts/make-icons.js",
    "install-app": "node ./etc/scripts/package.js --install"
  },
  "build": {
    "appId": "com.densebrain.epictask",
    "copyright": "Copyright Â© 2016 Densebrain, Inc.",
    "productName": "Epictask",
    "squirrelWindows": {
      "msi": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerHeaderIcon": "build/icon.ico"
    },
    "linux": {
      "category": "Development",
      "packageCategory": "devel",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "mac": {
      "identity": "Densebrain, Inc",
      "icon": "build/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "extraFiles": [
      "build/**/*"
    ],
    "files": [
      "build/**/*",
      "etc/packages/**/*",
      "dist/app/**/*",
      "package.json",
      "node_modules/**/*",
      "!**/node_modules/*/{README.md,README,readme.md,readme,test}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.idea,appveyor.yml,.travis.yml,circle.yml}"
    ]
  },
  "directories": {
    "app": ".",
    "buildResources": "build",
    "output": "dist/build"
  },
  "packaging": {
    "destination": "./target/packages",
    "platforms": [
      "darwin-x64"
    ],
    "platformResources": {
      "darwin": {
        "icon": "./build/icon.icns"
      }
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "browser": {
    "vertx": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/densebrain/epictask"
  },
  "author": {
