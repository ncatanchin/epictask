{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": false,
		"emitDecoratorMetadata": true,
		"jsx": "react",
		"experimentalDecorators": true,
		"preserveConstEnums": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"outDir": "dist/tsc",
		"baseUrl": "./",
		"paths": {
			"epictask/*": [
				"./src/epictask/*"
			],
			"ui": [
				"./src/epictask/ui/index.ts"
			],
			"ui/*": [
				"./src/epictask/ui/*"
			],
			"shared": [
				"./src/epictask/shared/index.ts"
			],
			"shared/*": [
				"./src/epictask/shared/*"
			],
			"main": [
				"./src/epictask/main/index.ts"
			],
			"main/*": [
				"./src/epictask/main/*"
			],
			"components": [
				"./src/epictask/ui/components/index.ts"
			],
			"components/*": [
				"./src/epictask/ui/components/*"
			],
			"typedux": [
				"./libs/typedux/src/index.ts"
			],
			"typedux/*": [
				"./libs/typedux/src/*"
			],
			"typemutant": [
				"./libs/typemutant/src/index.ts"
			],
			"typemutant/*": [
				"./libs/typemutant/src/*"
			],
			"typelogger": [
				"./libs/typelogger/src/index.ts"
			],
			"typelogger/*": [
				"./libs/typelogger/src/*"
			],
			"typestore": [
				"./libs/typestore/packages/typestore/src/index.ts"
			],
			"typestore/*": [
				"./libs/typestore/packages/typestore/src/*"
			],
			"typestore-mocks": [
				"./libs/typestore/packages/typestore-mocks/src/index.ts"
			],
			"typestore-mocks/*": [
				"./libs/typestore/packages/typestore-mocks/src/*"
			],
			"typestore-plugin-pouchdb": [
				"./libs/typestore/packages/typestore-plugin-pouchdb/src/index.ts"
			],
			"typestore-plugin-pouchdb/*": [
				"./libs/typestore/packages/typestore-plugin-pouchdb/src/*"
			]
		}
	},
	"exclude": [
		"**/libs/**/*.d.ts",
		"libs/typestore/typings/browser.d.ts",
		"libs/typestore/typings/main.d.ts",
		"./libs/typestore/typings",
		"./libs/typestore/typings/browser",
		"./libs/typestore/typings/main",
		"**/libs/typestore/typings/**/*.*",
		"libs/typestore/packages/typestore/typings",
		"libs/typestore/packages/typestore-mocks/typings",
		"libs/typestore/packages/typestore-plugin-pouchdb/typings",
		"libs/typestore/packages/typestore-plugin-indexeddb/typings",
		"libs/typelogger/typings",

		"libs/typemutant/typings",
		"libs/typedux/typings",
		"node_modules",
		"node_modules/**/*.*",
		"**/node_modules",
		"**/node_modules/**/*.*",
		"**/dist",
		"**/dist/**/*.*",
		"dist",
		"logs",
		"etc",
		".awcache"
	]
}
