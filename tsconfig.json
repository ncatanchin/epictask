{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": false,
		"emitDecoratorMetadata": true,
		"jsx": "react",
		"experimentalDecorators": true,
		"preserveConstEnums": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"baseUrl": "./src",
		"outDir": "./dist/tsc",
		"paths": {
			"epictask/*": [
				"./epictask/*"
			],
			"app": [
				"./epictask/app/index.ts"
			],
			"app/*": [
				"./epictask/app/*"
			],
			"shared": [
				"./epictask/shared/index.ts"
			],
			"shared/*": [
				"./epictask/shared/*"
			],
			"main": [
				"./epictask/main/index.ts"
			],
			"main/*": [
				"./epictask/main/*"
			],
			"components": [
				"./epictask/app/components/index.ts"
			],
			"components/*": [
				"./epictask/app/components/*"
			]
		}
	},
	"exclude": [
		"node_modules",
		"dist"
	],
	"filesGlob": [
		"typings/index.d.ts",
		"src/**/*.ts",
		"src/**/*.tsx"
	],
