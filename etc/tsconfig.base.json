{
	"compilerOptions": {
		"target": "ES6",
		"lib": [
			"es7",
			"es2015",
			"es2016",
			"es2017",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": false,
		"emitDecoratorMetadata": true,
		"jsx": "react",
		"experimentalDecorators": true,
		"preserveConstEnums": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"outDir": "../dist/tsc",
		"paths": {
			"tests/*": [
				"./tests/*"
			],
			"ui": [
				"./ui/index.ts"
			],
			"ui/*": [
				"./ui/*"
			],
			"jobs/*": [
				"./jobs/*"
			],

			"GitHubClient": [
				"./shared/GitHubClient.ts"
			],
			"Constants": [
				"./shared/Constants.ts"
			],

			"Settings": [
				"./shared/Settings.ts"
			],
			"shared": [
				"./shared/index.ts"
			],
			"shared/*": [
				"./shared/*"
			],
			"main": [
				"./main/index.ts"
			],
			"main/*": [
				"./main/*"
			],
			"components": [
				"./ui/components/index.ts"
			],
			"components/*": [
				"./ui/components/*"
			],
			"typelogger": [
				"../../typelogger/src/index.ts"
			],
			"typedux": [
				"../../typedux/src/index.ts"
			],
			"typestore": [
				"../../typestore/packages/typestore/src/index.ts"
			],
			"typestore-plugin-pouchdb": [
				"../../typestore/packages/typestore-plugin-pouchdb/src/index.ts"
			],
			"typestore-mocks": [
				"../../typestore/packages/typestore-mocks/src/index.ts"
			]
			

		}
	},
	"exclude": [
		"libs/**/typings",
		"libs/**/package.json",
		"libs/**/jsconfig.json",
		"libs/**/*.spec.ts",
		"libs/**/tsconfig.json",
		"libs/**/test",
		"libs/**/node_modules",
		"dist",
		"target",
		"etc",
		"bin",
		".awcache",
		".happy**/*.*",
		"node_modules",
		"node_modules.disk"
	]
}
