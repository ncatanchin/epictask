{
	"compilerOptions": {
		"target": "ES6",
		"lib": [
			"es7",
			"es2015",
			"es2016",
			"es2017",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": false,
		"emitDecoratorMetadata": true,
		"jsx": "react",
		"experimentalDecorators": true,
		"preserveConstEnums": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"outDir": "../dist/tsc",
		"paths": {
			"epictask/*": [
				"./*"
			],
			"tests/*": [
				"./tests/*"
			],
			"ui": [
				"./ui/index.ts"
			],
			"ui/*": [
				"./ui/*"
			],
			"actions": [
				"./shared/actions/index.ts"
			],
			"actions/*": [
				"./shared/actions/*"
			],
			"util": [
				"./shared/util/index.ts"
			],
			"util/*": [
				"./shared/util/*"
			],
			"models": [
				"./shared/models/index.ts"
			],
			"models/*": [
				"./shared/models/*"
			],
			"GitHubClient": [
				"./shared/GitHubClient.ts"
			],
			"Constants": [
				"./shared/Constants.ts"
			],

			"Settings": [
				"./shared/Settings.ts"
			],
			"shared": [
				"./shared/index.ts"
			],
			"shared/*": [
				"./shared/*"
			],
			"main": [
				"./main/index.ts"
			],
			"main/*": [
				"./main/*"
			],
			"components": [
				"./ui/components/index.ts"
			],
			"components/*": [
				"./ui/components/*"
			],
			"typedux": [
				"../libs/typedux/src/index.ts"
			],
			"typedux/*": [
				"../libs/typedux/src/*"
			],
			"typemutant": [
				"../libs/typemutant/src/index.ts"
			],
			"typemutant/*": [
				"../libs/typemutant/src/*"
			],
			"typelogger": [
				"../libs/typelogger/src/index.ts"
			],
			"typelogger/*": [
				"../libs/typelogger/src/*"
			],
			"typestore": [
				"../libs/typestore/packages/typestore/src/index.ts"
			],
			"typestore/*": [
				"../libs/typestore/packages/typestore/src/*"
			],
			"typestore-mocks": [
				"../libs/typestore/packages/typestore-mocks/src/index.ts"
			],
			"typestore-mocks/*": [
				"../libs/typestore/packages/typestore-mocks/src/*"
			],
			"typestore-plugin-pouchdb": [
				"../libs/typestore/packages/typestore-plugin-pouchdb/src/index.ts"
			],
			"typestore-plugin-pouchdb/*": [
				"../libs/typestore/packages/typestore-plugin-pouchdb/src/*"
			]

		}
	},
	"exclude": [
		"libs/**/typestore-example-node",
		"libs/**/typestore-example-webpack",
		"libs/**/typestore-plugin-cloudsearch",
		"libs/**/typestore-plugin-dynamodb",
		"libs/**/typings",
		"libs/**/package.json",
		"libs/**/jsconfig.json",
		"libs/**/*.spec.ts",
		"libs/**/tsconfig.json",
		"libs/**/test",
		"node_modules"
	]
}
